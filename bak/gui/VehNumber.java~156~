        if (Number.length() > 6) {
            Number = Number.substring(0, 6);
        }
        jLabel1.setText(Number);
    }

    public void jButton_U_actionPerformed(ActionEvent e) {
        String Number = "";
        Number = jLabel1.getText();
        Number = Number + "U";
        if (Number.length() > 6) {
            Number = Number.substring(0, 6);
        }
        jLabel1.setText(Number);
    }

    public void jButton_V_actionPerformed(ActionEvent e) {
